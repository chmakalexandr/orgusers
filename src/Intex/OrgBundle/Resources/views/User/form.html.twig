{% extends 'IntexOrgBundle::layout.html.twig' %}

{% block namepage %}
    <h3>
        {% trans %} Add user to organization {{ company.name}} {% endtrans %}
    </h3>
{% endblock %}

{% block body %}
    {{ form_start(form, { 'action': path('intex_org_user_create',{'companyId':company.id}), 'method': 'POST', 'attr': {'class': 'userform'} }) }}
    {{ form_widget(form) }}
    <p>
        <input type="submit" value="Submit" class="btn btn-default">
    </p>
{% endblock %}